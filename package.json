{
  "name": "sidecar-indexer",
  "private": true,
  "scripts": {
    "build": "pnpm run typegen && pnpm run codegen && rm -rf lib && tsc",
    "start": "node --require=dotenv/config lib/main.js",
    "typegen": "squid-evm-typegen src/abi abi/StateOracle.json",
    "codegen": "squid-typeorm-codegen",
    "migration:generate": "squid-typeorm-migration generate",
    "migration:apply": "squid-typeorm-migration apply",
    "migration:reset": "rm -rf db/migrations && pnpm run build && pnpm run migration:generate && pnpm run migration:apply"
  },
  "engines": {
    "node": ">=22.14.0"
  },
  "packageManager": "pnpm@10.30.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "keccak"
    ]
  },
  "dependencies": {
    "@subsquid/evm-abi": "^0.3.1",
    "@subsquid/evm-codec": "^0.3.0",
    "@subsquid/evm-processor": "^1.28.0",
    "@subsquid/logger": "^1.4.0",
    "@subsquid/typeorm-migration": "^1.3.0",
    "@subsquid/typeorm-store": "^1.7.0",
    "dotenv": "^16.4.5",
    "pg": "^8.18.0",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@subsquid/evm-typegen": "^4.6.0",
    "@subsquid/typeorm-codegen": "^2.0.2",
    "@types/node": "^22.14.0",
    "typescript": "~5.5.2"
  }
}
